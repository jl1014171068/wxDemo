.my-top{
  background: #ea5503;
  width: 100%;
  height:400rpx;
}
.user-info{
  display: flex;
  align-items: center;
  flex-flow: column;
  padding: 50rpx 0 0 0;
}
.userinfo-avatar{
  width: 180rpx;
  height:180rpx;
  border-radius: 100%;
}
.menu-list{
  width: 84%;
  margin: -30px auto 0;
  background: #fff;
  border-radius: 10rpx;
  padding:20rpx 30rpx;
}
.menu-list .item .text{
  color:#999;
  font-size: 24rpx;
  margin-left: 20rpx;
  padding:20rpx 0;
}
.menu-list .item{
  border-bottom: #eee solid 1px;
  display: flex;
  align-items: center;
}